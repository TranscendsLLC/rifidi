#!/bin/sh
/usr/bin/java -Declipse.ignoreApp=true -Dosgi.noShutdown=true -Dorg.rifidi.edge.configuration=config/rifidi.xml -Dosgi.clean=true -Dosgi.console=2020 -Dorg.osgi.framework.bootdelegation=javax.xml.ws -Dcom.sun.management.jmxremote.port=2021 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Djava.rmi.server.hostname=127.0.0.1 -Dorg.rifidi.edge.core.rmi.port=1101 -Dorg.osgi.service.http.port=8091 -Dosgi.framework.extensions=org.eclipse.equinox.weaving.hook -Dorg.aspectj.osgi.verbose=true -Dorg.rifidi.edge.autostart=true -Dorg.rifidi.edge.logging=config/logging.properties -Dorg.rifidi.ui.notify=true -Dorg.rifidi.mqtt.enabled=true -Dorg.rifidi.ale.enabled=true -Dorg.rifidi.ale.port=8112 -jar /usr/local/sbin/rifidi/plugins/org.eclipse.equinox.launcher_1.3.100.v20150511-1540.jar -launcher /usr/local/sbin/rifidi/rifidi-server -name Rifidi-server --launcher.library /usr/local/sbin/rifidi/plugins/org.eclipse.equinox.launcher.gtk.linux.x86_1.1.300.v20150602-1417/eclipse_1612.so -exitdata 2c0024 -vm /usr/bin/java
