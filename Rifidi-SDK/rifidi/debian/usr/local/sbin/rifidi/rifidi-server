#!/bin/sh
/usr/bin/java -Declipse.ignoreApp=true -Dosgi.noShutdown=true -Dorg.rifidi.edge.configuration=config/rifidi.xml -Dosgi.clean=true -Dosgi.console=2020 -Dorg.osgi.framework.bootdelegation=javax.xml.ws -Dcom.sun.management.jmxremote.port=2021 -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Djava.rmi.server.hostname=127.0.0.1 -Dorg.rifidi.edge.core.rmi.port=1101 -Dorg.osgi.service.http.port=8091 -Dosgi.framework.extensions=org.eclipse.equinox.weaving.hook -Dorg.aspectj.osgi.verbose=true -Dorg.rifidi.edge.autostart=true -Dorg.rifidi.edge.logging=config/logging.properties -Dorg.rifidi.ui.notify=true -Dorg.rifidi.restlet.port=8111 -Dorg.rifidi.restlet.enabled=true -Dorg.rifidi.mqtt.enabled=true -Dorg.rifidi.llrp.encode.targetepc=000000000000000000000000 -Dorg.rifidi.llrp.encode.tagmask=000000000000000000000000 -Dorg.rifidi.llrp.encode.timeout=10000 -Dorg.rifidi.llrp.encode.accesspwd=0 -Dorg.rifidi.llrp.encode.oldaccesspwd=0 -Dorg.rifidi.llrp.encode.killpwd=0 -Dorg.rifidi.llrp.encode.killpwdlockprivilege=Read_Write -Dorg.rifidi.llrp.encode.accesspwdlockprivilege=Read_Write -Dorg.rifidi.llrp.encode.epclockprivilege=Read_Write -Dorg.rifidi.llrp.encode.mqttqos -Dorg.rifidi.llrp.encode.mqttbroker -Dorg.rifidi.llrp.encode.mqttclientid=llrpEncodeId  -Dorg.rifidi.restlet.ssl.port=8183 -Dorg.rifidi.restlet.ssl.enabled=false -Dorg.rifidi.restlet.ssl.keystorepath=\config\serverX.jks -Dorg.rifidi.restlet.ssl.keystorepassword=password -Dorg.rifidi.restlet.ssl.keypassword=password -Dorg.rifidi.restlet.ssl.keystoretype=JKS -jar /usr/local/sbin/rifidi/plugins/org.eclipse.equinox.launcher_1.3.0.v20130327-1440.jar -launcher /usr/local/sbin/rifidi/rifidi-server -name Rifidi-server --launcher.library /usr/local/sbin/rifidi/plugins/org.eclipse.equinox.launcher.gtk.linux.x86_1.1.200.v20140116-2212/eclipse_1508.so -exitdata 2c0024 -vm /usr/bin/java
