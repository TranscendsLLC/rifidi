<DYNAMIC_SWT_FORM NAME="AlienReaderInfo">
	<STRING>
		<ELEMENT_NAME>ipAddress</ELEMENT_NAME>
		<DISPLAY_NAME>IP Address</DISPLAY_NAME>
		<DEFAULT_VALUE>127.0.0.1</DEFAULT_VALUE>
		<EDITABLE>true</EDITABLE>
		<REGEX>(.)+</REGEX>
	</STRING>
	<INTEGER>
		<ELEMENT_NAME>port</ELEMENT_NAME>
		<DISPLAY_NAME>Port</DISPLAY_NAME>
		<DEFAULT_VALUE>23</DEFAULT_VALUE>
		<EDITABLE>true</EDITABLE>
		<MIN>0</MIN>
		<MAX>65535</MAX>
	</INTEGER>
	<STRING>
		<ELEMENT_NAME>username</ELEMENT_NAME>
		<DISPLAY_NAME>User Name</DISPLAY_NAME>
		<DEFAULT_VALUE>alien</DEFAULT_VALUE>
		<EDITABLE>true</EDITABLE>
		<REGEX>(.)+</REGEX>
	</STRING>
	<STRING>
		<ELEMENT_NAME>password</ELEMENT_NAME>
		<DISPLAY_NAME>Password</DISPLAY_NAME>
		<DEFAULT_VALUE>password</DEFAULT_VALUE>
		<EDITABLE>true</EDITABLE>
		<REGEX>(.)+</REGEX>
	</STRING>
	<INTEGER>
		<ELEMENT_NAME>reconnectionInterval</ELEMENT_NAME>
		<DISPLAY_NAME>Reconnect Interval</DISPLAY_NAME>
		<DEFAULT_VALUE>1000</DEFAULT_VALUE>
		<EDITABLE>true</EDITABLE>
		<MIN>0</MIN>
		<MAX>2147483647</MAX>
	</INTEGER>
	<INTEGER>
		<ELEMENT_NAME>maxNumConnectionsAttempts</ELEMENT_NAME>
		<DISPLAY_NAME>Connection Attempts</DISPLAY_NAME>
		<DEFAULT_VALUE>3</DEFAULT_VALUE>
		<EDITABLE>true</EDITABLE>
		<MIN>-1</MIN>
		<MAX>2147483647</MAX>
	</INTEGER>
</DYNAMIC_SWT_FORM>