         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.rifidi.edge.app.Application">
         </run>
      </application>
   </extension>
   <extension
         id="EdgeServer"
         point="org.eclipse.core.runtime.products">
